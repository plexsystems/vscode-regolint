{
  "name": "vscode-regolint",
  "displayName": "Rego Lint",
  "description": "Lint your Rego files",
  "license": "MIT",
  "homepage": "https://github.com/plexsystems/vscode-regolint",
  "version": "0.1.0",
  "publisher": "plex",
  "icon": "icon/rego_lint_128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/plexsystems/vscode-regolint.git"
  },
  "bugs": {
    "url": "https://github.com/plexsystems/vscode-regolint/issues"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Linters"
  ],
  "keywords": [
    "rego",
    "lint"
  ],
  "activationEvents": [
    "onLanguage:rego"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "precompile": "rimraf ./out",
    "watch": "tsc -watch -p ./",
    "test": "npm run compile && mocha out/test/*.test.js"
  },
  "devDependencies": {
    "@types/mocha": "7.0.1",
    "@types/node": "13.7.4",
    "@types/vscode": "1.42.0",
    "mocha": "7.0.1",
    "rimraf": "3.0.2",
    "typescript": "3.8.2",
    "vscode": "1.1.6"
  }
}
